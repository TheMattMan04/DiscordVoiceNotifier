{
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      false
    ],
    "no-console": false,
    "object-literal-sort-keys": false,
    "triple-equals": false,
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      {
        "limit": 180,
        "ignore-pattern": "RegExp"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "no-bitwise": false
  },
  "rulesDirectory": []
}
